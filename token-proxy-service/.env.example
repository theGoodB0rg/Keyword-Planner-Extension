# Server
PORT=8787
NODE_ENV=development
# Shared secret the extension sends in X-EXT-SECRET header
EXT_SHARED_SECRET=replace-me

# Google Trends proxy safeguards
# Maximum number of trend requests allowed per client within the window (defaults to 60 when unset)
TRENDS_RATE_LIMIT=
# Rolling window size in milliseconds for the rate limit (defaults to 60000ms / 1 minute when unset)
TRENDS_RATE_WINDOW_MS=

# Keys (PEM or JWK)
# If not provided, a random P-256 keypair is generated at startup and JWKS endpoint exposes public key
PRIVATE_JWK={}
PUBLIC_JWK={}

# Provider API keys (optional)
OPENAI_API_KEY=
